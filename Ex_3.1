#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, i, max;
    printf("Введите количесво чисел: ");
    scanf("%d", &n);
    int num[n];
    for(i = 0; i < n; i++)
    {
        printf("Введите %d-е число: ", i + 1);
        scanf("%d", &num[i]);
    }
    max = num[0];
    for(i = 0; i < n; i++)
    {
        if(num[i] > max)
        {
            max = num[i];
        }
    }
    printf("Максимальное число равно %d.", max);
}
