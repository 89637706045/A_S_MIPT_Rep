#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, n, f, ff = 1, fff = 1;
    printf("Введите номер числа Фибоначчи: ");
    scanf("%d", &n);
    if(n <= 2)
    {
        f = 1;
    }
    else
    {
        for(i = 3; i <= n; i++)
        {
            f = ff + fff;
            fff = ff;
            ff = f;
        }
    }
    printf("%d-е чило Фибоначчи равно %d.", n, f);
    return 0;
}
